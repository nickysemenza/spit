{
  "name": "spit",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "npm": ">=4"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "dashboard": "webpack-dashboard -- npm start",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src backend tools --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js src/**/*.spec.js backend/**/*.spec.js --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html mocha -- --require ./tools/testSetup.js src/**/*.spec.js  backend/**/*.spec.js --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly mocha -- --require ./tools/testSetup.js src/**/*.spec.js && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && npm run codecov",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "codecov": "cat ./coverage/lcov.info | codecov"
  },
  "author": "Nicky Semenza",
  "license": "MIT",
